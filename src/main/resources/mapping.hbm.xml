<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="classes.Movie" table="Movie">
        <meta attribute="class-description">
            This class contains the movie detail.
        </meta>
        <id name="id" type="int" column="movie_id">
            <generator class="native"/>
        </id>
        <property name="filmwebId" column="filmweb_id" type="long"/>
        <property name="originalTitle" column="foreign_title" type="string"/>
        <property name="polishTitle" column="polish_title" type="string"/>
        <property name="image_0" column="image_0" type="string"/>
        <property name="image_1" column="image_1" type="string"/>
        <property name="image_2" column="image_2" type="string"/>
        <property name="image_3" column="image_3" type="string"/>
        <property name="image_4" column="image_4" type="string"/>
        <property name="image_5" column="image_5" type="string"/>
        <property name="image_6" column="image_6" type="string"/>
        <property name="year" column="production_year" type="string"/>
        <property name="cast" column="cast" type="string"/>
        <property name="duration" column="duration" type="string"/>
        <property name="productionCountry" column="production_country" type="string"/>
        <property name="filmwebGenre" column="filmweb_genre" type="string"/>
        <property name="description" column="description" type="string"/>
        <property name="plot" column="plot" type="string"/>
        <property name="genre" column="genre" type="string"/>
        <property name="column" column="position_column" type="int"/>
        <property name="row" column="position_row" type="int"/>
        <property name="countryType" column="country_type" type="string"/>
    </class>

    <class name="classes.Series" table="Series">
        <meta attribute="class-description">
            This class contains the series detail.
        </meta>
        <id name="id" type="int" column="series_id">
            <generator class="native"/>
        </id>
        <property name="filmwebId" column="filmweb_id" type="long"/>
        <property name="originalTitle" column="foreign_title" type="string"/>
        <property name="polishTitle" column="polish_title" type="string"/>
        <property name="image_0" column="image_0" type="string"/>
        <property name="image_1" column="image_1" type="string"/>
        <property name="image_2" column="image_2" type="string"/>
        <property name="image_3" column="image_3" type="string"/>
        <property name="image_4" column="image_4" type="string"/>
        <property name="image_5" column="image_5" type="string"/>
        <property name="image_6" column="image_6" type="string"/>
        <property name="year" column="production_year" type="string"/>
        <property name="cast" column="cast" type="string"/>
        <property name="duration" column="duration" type="string"/>
        <property name="productionCountry" column="production_country" type="string"/>
        <property name="filmwebGenre" column="filmweb_genre" type="string"/>
        <property name="description" column="description" type="string"/>
        <property name="plot" column="plot" type="string"/>
        <property name="genre" column="genre" type="string"/>
        <property name="column" column="position_column" type="int"/>
        <property name="row" column="position_row" type="int"/>
        <property name="countryType" column="country_type" type="string"/>
        <property name="numberOfEpisodes" column="number_of_episodes" type="string"/>
        <property name="numberOfSeasons" column="number_of_seasons" type="string"/>
    </class>
</hibernate-mapping>